<h1>Edit Contact</h1>
<%= form_tag "/contacts/#{@contact.id}", method: :patch do %>
  <div class="form-group">First Name: <input type="text" name="first_name" value="<%= @contact.first_name %>"></div>
  <div class="form-group">Last Name: <input type="text" name="last_name" value="<%= @contact.last_name %>"></div>
  <div class="form-group">Email: <input type="text" name="email" value="<%= @contact.email %>"></div>
  <div class="form-group">Phone Number: <input type="text" name="phone_number" value="<%= @contact.phone_number %>"></div>
  <div class="form-group"><p><input type="submit" class="btn btn-primary btn-md" value="Update Contact"> <a href="/contacts/<%= @contact.id %>" class="btn btn-default btn-md">Cancel</a></p></div>
<% end %>